{
  "version": 2,
  "name": "iudex",
  "builds": [
    { "src": "web/next.config.js", "use": "@now/next" },
    {
      "src": "api/index.ts",
      "use": "@now/node-server"
    }
  ],
  "routes": [
    {
      "src": "/graphql",
      "dest": "/api/index.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/web/$1"
    }
  ]
}
